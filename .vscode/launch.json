{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Attach to Node",
      "remoteRoot": "/usr/src/app"
    },

    {
      "name": "Run npm preinstall",
      "type": "f5anything",
      "request": "launch",
      "command": "cd .\\src; npm i"
    },
    {
      "command": "docker-compose up",
      "name": "Run server",
      "request": "launch",
      "type": "f5anything"
    },
    {
      "command": "docker-compose build",
      "name": "Build docker-compose",
      "request": "launch",
      "type": "f5anything"
    },
    {
      "command": "docker build . -t ristaltv/os-lab1",
      "name": "Build docker",
      "request": "launch",
      "type": "f5anything"
    },
    {
      "command": "docker run -p 8080:8080 ristaltv/os-lab1 ",
      "name": "Run docker",
      "request": "launch",
      "type": "f5anything"
    },
    {
      "command": "cd .\\src; npm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickMatchedProcess}",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
